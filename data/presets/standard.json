      "pipelines": [
        {
          "id": "standard-pipeline",
          "name": "Standard Pipeline",
          "description": "A balanced 3-phase workflow with context, draft, and review phases.",
          "version": "2.0.0",
          "threads": {
            "main": {
              "id": "main",
              "name": "Main",
              "description": "Primary deliberation space",
              "visible": true
            },
            "context": {
              "id": "context",
              "name": "Context",
              "description": "Context gathering and analysis",
              "visible": true
            },
            "drafting": {
              "id": "drafting",
              "name": "Drafting",
              "description": "Draft development",
              "visible": true
            }
          },
          "phases": [
            {
              "id": "phase_1_context",
              "name": "Phase 1: Context Gathering",
              "description": "Gather and analyze relevant story context",
              "icon": "magnifying-glass",
              "teams": [
                "curation_team",
                "plot_team"
              ],
              "threads": {
                "phaseThread": {
                  "enabled": true
                },
                "teamThreads": {
                  "context": true
                }
              },
              "actions": [
                {
                  "id": "gather_context",
                  "name": "Gather Context",
                  "actionType": "standard",
                  "description": "Retrieve relevant story information",
                  "participants": {
                    "positionIds": [
                      "curation_lead"
                    ],
                    "orchestration": "sequential"
                  },
                  "promptTemplate": "Review the current story context and identify key information relevant to the upcoming response.\n\nContext:\n{{context}}\n\nUser Input:\n{{input}}\n\nSummarize the most relevant story elements, character states, and recent events that should inform the response."
                },
                {
                  "id": "continuity_check",
                  "name": "Continuity Check",
                  "actionType": "standard",
                  "description": "Verify continuity considerations",
                  "participants": {
                    "positionIds": [
                      "plot_lead"
                    ],
                    "orchestration": "sequential"
                  },
                  "promptTemplate": "Review the gathered context and user input for continuity considerations.\n\nGathered Context:\n{{previousAction.output}}\n\nUser Input:\n{{input}}\n\nIdentify any continuity points that must be maintained and potential issues to avoid."
                }
              ],
              "output": {
                "consolidation": "synthesize"
              }
            },
            {
              "id": "phase_2_draft",
              "name": "Phase 2: Draft",
              "description": "Write the narrative draft with the full prose team",
              "icon": "pen-nib",
              "teams": [
                "prose_team"
              ],
              "threads": {
                "phaseThread": {
                  "enabled": true
                },
                "teamThreads": {
                  "drafting": true
                }
              },
              "actions": [
                {
                  "id": "write_draft",
                  "name": "Write Draft",
                  "actionType": "standard",
                  "description": "Create the initial narrative draft",
                  "participants": {
                    "teamIds": [
                      "prose_team"
                    ],
                    "orchestration": "sequential"
                  },
                  "promptTemplate": "Write a narrative response based on the context and continuity analysis.\n\nContext Analysis:\n{{previousPhase.output}}\n\nUser Input:\n{{input}}\n\nWrite an engaging, well-crafted narrative response that:\n1. Advances the story naturally\n2. Maintains established tone and style\n3. Respects continuity considerations\n4. Features vivid prose and natural dialogue"
                }
              ],
              "output": {
                "consolidation": "last"
              }
            },
            {
              "id": "phase_3_review",
              "name": "Phase 3: Review",
              "description": "Review, polish, and finalize the response",
              "icon": "check-double",
              "teams": [
                "prose_team",
                "plot_team"
              ],
              "threads": {
                "phaseThread": {
                  "enabled": true
                },
                "teamThreads": {}
              },
              "actions": [
                {
                  "id": "continuity_review",
                  "name": "Continuity Review",
                  "actionType": "standard",
                  "description": "Review for consistency issues",
                  "participants": {
                    "positionIds": [
                      "plot_lead"
                    ],
                    "orchestration": "sequential"
                  },
                  "promptTemplate": "Review this draft for continuity issues.\n\nDraft:\n{{previousPhase.output}}\n\nContext:\n{{context}}\n\nIdentify any contradictions with established facts, timeline issues, or character inconsistencies. List specific issues or confirm the draft is consistent."
                },
                {
                  "id": "editorial_polish",
                  "name": "Editorial Polish",
                  "actionType": "standard",
                  "description": "Final editorial pass",
                  "participants": {
                    "positionIds": [
                      "prose_lead"
                    ],
                    "orchestration": "sequential"
                  },
                  "promptTemplate": "Review and polish this draft, addressing any continuity notes.\n\nDraft:\n{{previousPhase.output}}\n\nContinuity Notes:\n{{previousAction.output}}\n\nFix any issues noted. Polish the prose for clarity, flow, and engagement. Return the final polished response."
                }
              ],
              "output": {
                "consolidation": "last"
              }
            }
          ],
          "staticContext": {
            "includeCharacterCard": true,
