  "pipelines": [
    {
      "id": "quick-pipeline",
      "name": "Quick Pipeline",
      "description": "A streamlined 4-phase workflow for fast narrative generation.",
      "version": "2.0.0",
      "threads": {
        "main": {
          "id": "main",
          "name": "Main",
          "description": "Primary workspace for the quick pipeline",
          "visible": true
        },
        "drafting": {
          "id": "drafting",
          "name": "Drafting",
          "description": "Draft development thread",
          "visible": true
        }
      },
      "phases": [
        {
          "id": "phase_1_context",
          "name": "Gather Context",
          "description": "Analyze input and gather relevant context",
          "icon": "üìã",
          "teams": ["plot_team"],
          "threads": {
            "phaseThread": { "enabled": true },
            "teamThreads": {}
          },
          "actions": [
            {
              "id": "analyze_input",
              "name": "Analyze Input",
              "actionType": "standard",
              "description": "Understand the user's input and story context",
              "participants": {
                "positionIds": ["plot_lead"],
                "orchestration": "sequential"
              },
              "promptTemplate": "Analyze the following input and current story context. Identify key elements, character positions, and any continuity considerations.\n\nContext:\n{{context}}\n\nUser Input:\n{{input}}\n\nProvide a brief analysis (2-3 sentences) highlighting what needs to be addressed in the response."
            }
          ],
          "output": {
            "consolidation": "last"
          }
        },
        {
          "id": "phase_2_draft",
          "name": "Generate Draft",
          "description": "Create the initial narrative draft",
          "icon": "‚úçÔ∏è",
          "teams": ["prose_team"],
          "threads": {
            "phaseThread": { "enabled": true },
            "teamThreads": { "prose": true }
          },
          "actions": [
            {
              "id": "create_draft",
              "name": "Create Draft",
              "actionType": "standard",
              "description": "Write the narrative response",
              "participants": {
                "positionIds": ["prose_lead"],
                "orchestration": "sequential"
              },
              "promptTemplate": "Based on the context and analysis, write a narrative response.\n\nContext:\n{{context}}\n\nAnalysis:\n{{previousPhase.output}}\n\nUser Input:\n{{input}}\n\nWrite an engaging narrative response that advances the story naturally. Match the established tone and style."
            }
          ],
          "output": {
            "consolidation": "last"
          }
        },
        {
          "id": "phase_3_review",
          "name": "Quick Review",
          "description": "Fast editorial review and polish",
          "icon": "üîç",
          "teams": ["prose_team"],
          "threads": {
            "phaseThread": { "enabled": true },
            "teamThreads": {}
          },
          "actions": [
            {
              "id": "editorial_review",
              "name": "Editorial Review",
              "actionType": "standard",
              "description": "Review and polish the draft",
              "participants": {
                "positionIds": ["prose_member"],
                "orchestration": "sequential"
              },
              "promptTemplate": "Review and polish the following draft. Fix any issues with clarity, grammar, or flow. Make minimal but impactful improvements.\n\nDraft:\n{{previousPhase.output}}\n\nProvide the polished version directly, without commentary."
            }
          ],
          "output": {
            "consolidation": "last"
          }
        },
        {
          "id": "phase_4_final",
          "name": "Final Output",
          "description": "Deliver the final response",
          "icon": "‚úÖ",
          "actions": [
            {
              "id": "deliver_response",
              "name": "Deliver Response",
              "actionType": "deliverables",
              "description": "Output the final narrative response",
              "promptTemplate": "{{previousPhase.output}}"
            }
          ],
          "output": {
            "consolidation": "last"
          }
        }
